<!DOCTYPE html>
<html>



<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
<script>

function test(){
	$.ajax({
		url:"/TodoTeamProject/mainData",
		dataType:"json",
		success:function(data){
			output='';
			$.each(data,function(){
				output+= '<span>';
				output+= '<h3>'+ this.id + '</h3>';
				output+= '<p>' + this.contents +'</p>';
				output+= '</span>';
			});
			document.getElementById('disp').innerHTML = output;
		}
	});
};



        </script>
</head>
<body>

<button id="ajax" onclick="test()">ajax כ</button></br>
<div id="disp"></div>



</body>
